{
  "name": "@projectsas/eventsource",
  "version": "1.0.9",
  "description": "EventSource client for Node.js and browser (polyfill)",
  "keywords": [
    "eventsource",
    "http",
    "streaming",
    "sse",
    "polyfill"
  ],
  "homepage": "http://github.com/projectsas/eventsource",
  "repository": "git://github.com/projectsas/eventsource.git",
  "directories": {
    "lib": "./lib"
  },
  "main": "./lib/eventsource",
  "license": "MIT",
  "devDependencies": {
    "buffer-from": "^1.1.1",
    "express": "^4.15.3",
    "mocha": "^3.5.3",
    "nyc": "^11.2.1",
    "serve-static": "^1.14.1",
    "ssestream": "^1.1.0",
    "standard": "^10.0.2",
    "webpack": "^3.5.6"
  },
  "scripts": {
